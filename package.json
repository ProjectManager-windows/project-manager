{
	"name": "project-manager",
	"productName": "project-manager",
	"description": "A foundation for scalable desktop apps",
	"keywords": [
		"electron",
		"boilerplate",
		"react",
		"typescript",
		"ts",
		"sass",
		"webpack",
		"hot",
		"reload"
	],
	"homepage": "https://github.com/electron-react-boilerplate/electron-react-boilerplate#readme",
	"bugs": {
		"url": "https://github.com/electron-react-boilerplate/electron-react-boilerplate/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/traineratwot/project-manager"
	},
	"license": "MIT",
	"author": {
		"name": "Traineratwot",
		"email": "Traineratwot@yandex.ru",
		"url": "https://Traineratwot.site"
	},
	"main": "./src/main/main.ts",
	"scripts": {
		"build": "concurrently \"yarn run build:main\" \"yarn run build:renderer\"",
		"build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
		"build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
		"postinstall": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
		"lint": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx",
		"package": "ts-node ./.erb/scripts/clean.js dist && yarn run build && electron-builder build --publish never",
		"prepare": "husky install",
		"rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
		"start": "ts-node ./.erb/scripts/check-port-in-use.js && yarn run start:renderer",
		"start:Interceptor": "ts-node ./src/main/cli/test.ts",
		"start:main": "cross-env NODE_ENV=development electronmon -r ts-node/register/transpile-only .",
		"start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
		"start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
		"test": "jest"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"cross-env NODE_ENV=development eslint --cache"
		],
		"*.json,.{eslintrc,prettierrc}": [
			"prettier --ignore-path .eslintignore --parser json --write"
		],
		"*.{css,scss}": [
			"prettier --ignore-path .eslintignore --single-quote --write"
		],
		"*.{html,md,yml}": [
			"prettier --ignore-path .eslintignore --single-quote --write"
		]
	},
	"browserslist": [],
	"prettier": {
		"singleQuote": true,
		"overrides": [
			{
				"files": [
					".prettierrc",
					".eslintrc"
				],
				"options": {
					"parser": "json"
				}
			}
		]
	},
	"jest": {
		"moduleDirectories": [
			"node_modules",
			"release/app/node_modules"
		],
		"moduleFileExtensions": [
			"js",
			"jsx",
			"ts",
			"tsx",
			"json"
		],
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
			"\\.(css|less|sass|scss)$": "identity-obj-proxy"
		},
		"setupFiles": [
			"./.erb/scripts/check-build-exists.ts"
		],
		"testEnvironment": "jsdom",
		"testEnvironmentOptions": {
			"url": "http://localhost/"
		},
		"testPathIgnorePatterns": [
			"release/app/dist"
		],
		"transform": {
			"\\.(ts|tsx|js|jsx)$": "ts-jest"
		}
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.1.1",
		"@fortawesome/free-brands-svg-icons": "^6.2.0",
		"@fortawesome/free-solid-svg-icons": "^6.1.1",
		"@fortawesome/react-fontawesome": "^0.2.0",
		"ace-builds": "^1.12.5",
		"body-parser": "^1.20.1",
		"bootstrap": "^5.1.3",
		"dayjs": "^1.11.6",
		"electron-debug": "^3.2.0",
		"electron-log": "^4.4.7",
		"electron-store": "^8.0.2",
		"electron-updater": "^5.0.3",
		"express": "^4.18.2",
		"file-icon-info": "^1.1.1",
		"glob": "^8.0.3",
		"i18next": "^21.8.13",
		"ignore": "^5.2.0",
		"jquery": "^3.6.1",
		"js-search": "^2.0.0",
		"json5": "^2.2.1",
		"libnpmsearch": "^6.0.0",
		"markdown-it": "^13.0.1",
		"md5": "^2.3.0",
		"npm-registry-fetch": "^14.0.2",
		"primeicons": "^5.0.0",
		"primereact": "^8.2.0",
		"react": "^18.1.0",
		"react-ace": "^10.1.0",
		"react-bootstrap": "^2.4.0",
		"react-dom": "^18.1.0",
		"react-i18next": "^11.18.0",
		"react-markdown": "^8.0.3",
		"react-resizable": "^3.0.4",
		"react-router-dom": "^6.3.0",
		"react-syntax-highlighter": "^15.5.0",
		"rehype-raw": "^6.1.1",
		"remark-gfm": "^3.0.1",
		"seedrandom": "^3.0.5",
		"toml": "^3.0.0",
		"xml-js": "^1.6.11"
	},
	"devDependencies": {
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.6",
		"@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.2.0",
		"@types/ejs": "^3.1.1",
		"@types/express": "^4.17.14",
		"@types/hidefile": "^3.0.0",
		"@types/jest": "^27.5.1",
		"@types/jquery": "^3.5.14",
		"@types/js-search": "^1.4.0",
		"@types/libnpmsearch": "^2.0.3",
		"@types/markdown-it": "^12.2.3",
		"@types/md5": "^2.3.2",
		"@types/node": "17.0.33",
		"@types/npm-registry-fetch": "^8.0.4",
		"@types/react": "^18.0.9",
		"@types/react-dom": "^18.0.4",
		"@types/react-resizable": "^3.0.3",
		"@types/react-syntax-highlighter": "^15.5.5",
		"@types/react-test-renderer": "^18.0.0",
		"@types/seedrandom": "^3.0.2",
		"@types/terser-webpack-plugin": "^5.0.4",
		"@types/webpack-bundle-analyzer": "^4.4.1",
		"@typescript-eslint/eslint-plugin": "^5.23.0",
		"@typescript-eslint/parser": "^5.23.0",
		"browserslist-config-erb": "^0.0.3",
		"chalk": "^4.1.2",
		"concurrently": "^7.1.0",
		"core-js": "^3.22.5",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.1",
		"css-minimizer-webpack-plugin": "^3.4.1",
		"detect-port": "^1.3.0",
		"electron": "^18.2.3",
		"electron-builder": "^23.0.3",
		"electron-devtools-installer": "^3.2.0",
		"electron-notarize": "^1.2.1",
		"electron-rebuild": "^3.2.7",
		"electronmon": "^2.0.2",
		"eslint": "^8.15.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-erb": "^4.0.3",
		"eslint-import-resolver-typescript": "^2.7.1",
		"eslint-import-resolver-webpack": "^0.13.2",
		"eslint-plugin-compat": "^4.0.2",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^26.2.2",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-react": "^7.29.4",
		"eslint-plugin-react-hooks": "^4.5.0",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.5.0",
		"husky": "^8.0.1",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^28.1.2",
		"jest-environment-jsdom": "^28.1.0",
		"lint-staged": "^12.4.1",
		"mini-css-extract-plugin": "^2.6.0",
		"prettier": "^2.6.2",
		"raw-loader": "^4.0.2",
		"react-refresh": "^0.13.0",
		"react-test-renderer": "^18.1.0",
		"rimraf": "^3.0.2",
		"sass": "^1.51.0",
		"sass-loader": "^12.6.0",
		"style-loader": "^3.3.1",
		"terser-webpack-plugin": "^5.3.1",
		"ts-jest": "^28.0.2",
		"ts-loader": "^9.3.0",
		"ts-node": "^10.8.2",
		"typescript": "^4.6.4",
		"url-loader": "^4.1.1",
		"webpack": "^5.72.1",
		"webpack-bundle-analyzer": "^4.5.0",
		"webpack-cli": "^4.9.2",
		"webpack-dev-server": "^4.9.0",
		"webpack-merge": "^5.8.0"
	},
	"build": {
		"productName": "Project-manager",
		"appId": "org.erb.ElectronReact",
		"asar": true,
		"asarUnpack": "**\\*.{node,dll,exe}",
		"files": [
			"dist",
			"node_modules",
			"package.json"
		],
		"afterSign": ".erb/scripts/notarize.js",
		"mac": {
			"target": {
				"target": "default",
				"arch": [
					"arm64",
					"x64"
				]
			},
			"type": "distribution",
			"hardenedRuntime": true,
			"entitlements": "assets/entitlements.mac.plist",
			"entitlementsInherit": "assets/entitlements.mac.plist",
			"gatekeeperAssess": false
		},
		"dmg": {
			"contents": [
				{
					"x": 130,
					"y": 220
				},
				{
					"x": 410,
					"y": 220,
					"type": "link",
					"path": "/Applications"
				}
			]
		},
		"win": {
			"target": [
				"nsis"
			]
		},
		"linux": {
			"target": [
				"AppImage"
			],
			"category": "Development"
		},
		"directories": {
			"app": "release/app",
			"buildResources": "assets",
			"output": "release/build"
		},
		"extraResources": [
			"./assets/**"
		],
		"publish": {
			"provider": "github",
			"owner": "project-manager",
			"repo": "project-manager"
		}
	},
	"collective": {
		"url": "https://opencollective.com/electron-react-boilerplate-594"
	},
	"devEngines": {
		"node": ">=14.x",
		"npm": ">=7.x"
	},
	"electronmon": {
		"patterns": [
			"!**/**",
			"src/main/*"
		],
		"logLevel": "quiet"
	}
}
